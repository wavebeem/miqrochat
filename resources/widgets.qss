/* vim: set ft=css: */

#theFriendStatus {
    background-color:    #88C5A7;
    border-top-color:    #73A78D;
    border-left-color:   #73A78D;
    border-bottom-color: #65937C;
    border-right-color:  #65937C;
    color: #222222;
    border-style: solid;
    border-width: 1px;
    padding: 2px;
    font-weight: bold;
    border-radius: 0px;
}

#theChatViewFrame {
    border-width: 1px;
    border-color: #444444;
    border-style: solid;
}
